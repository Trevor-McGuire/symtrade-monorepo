{
  "name": "symtrade-frontend",
  "private": true,
  "version": "0.0.0",
  "description": "Simulated market trading app with real time and historical trading capabilities.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "format": "prettier --write .",
    "test": "jest",
    "sdd": "node ./docs/assets/scripts/sdd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trevor-McGuire/Symtrade.git"
  },
  "keywords": [
    "simulated",
    "trading",
    "market",
    "real-time",
    "historical",
    "finance",
    "stocks",
    "investment",
    "portfolio"
  ],
  "author": "Trevor McGuire <tm@trevmcdev.com> (https://trevmcdev.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Trevor-McGuire/Symtrade/issues"
  },
  "homepage": "https://github.com/Trevor-McGuire/Symtrade#readme",
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "graphql": "^16.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.2.1",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "inquirer": "^9.2.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
